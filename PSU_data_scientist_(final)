    {
      "cell_type": "code",
      "source": [
        "# Create column \"Rank\"\n",
        "isi_eda_2['rank'] = isi_eda_2['citation_counts'].rank(ascending=False).astype(int)\n",
        "\n",
        "# Create a new order of columns\n",
