      "cell_type": "code",
      "source": [
        "df_scopus_sa = df_scopus[['FAC_ID', 'JOURNAL']]\n",
        "\n",
        "df_scopus_sa = df_scopus_sa.groupby('FAC_ID').count()\n",
        "\n",
        "df_scopus_sa.reset_index('FAC_ID', inplace=True)\n",
        "df_scopus_sa = df_scopus_sa.rename(columns={'JOURNAL': 'count_research_output'})\n",
        "\n",
        "# เพิ่ม column ใหม่เข้าไปใน DataFrame\n",
        "df_scopus_sa = df_scopus_sa.assign(journals_database = 'SCOPUS')"
