        "# Example case:  After the inspection with the team has been completed, it was instructed to replace the value with \"0\"\n",
        "df_scopus['ISSN'] = df_scopus['ISSN'].str.replace(r'X$', '0').fillna(np.nan)\n",
        "\n",
        "# Replace the escape character with 'z' string values\n",
        "df_scopus['ISSN'] = df_scopus['ISSN'].str.replace(r'^-$', 'z').fillna(np.nan)\n",
        "\n",
        "# Replace 'z' value in \"ISSN\" column to NaN values\n",
        "df_scopus['ISSN'] = df_scopus['ISSN'].replace('z', np.nan).fillna(np.nan)"
