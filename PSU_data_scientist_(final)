      "source": [
        "# Convert \"PUBLIC_DATE\" column to datetime format.\n",
        "df_scopus['PUBLIC_DATE2'] = pd.to_datetime(df_scopus['PUBLIC_DATE'], errors='coerce')"
      ],
      "metadata": {
        "id": "5LxMbXsaFC13"
      },
