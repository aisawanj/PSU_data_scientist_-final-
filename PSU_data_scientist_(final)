        "select_scopus_rso = df_scopus[['OUTPUT_YEAR', 'ARTICLE']]\n",
        "\n",
        "# Counting unique values \"ARTICLE\" column\n",
        "scopus_rso = select_scopus_rso.groupby('OUTPUT_YEAR').count()\n",
        "\n",
        "# Reset index columns\n",
        "scopus_rso.reset_index('OUTPUT_YEAR', inplace=True)\n",
