      "cell_type": "code",
      "source": [
        "# Replace missing values with Mode values\n",
        "df_isi['ISSN'] = df_isi['ISSN'].fillna(df_isi['ISSN'].mode()[0])\n",
        "df_isi['DOI'] = df_isi['DOI'].fillna(df_isi['DOI'].mode()[0])\n",
        "df_isi['USER_NAME'] = df_isi['USER_NAME'].fillna(df_isi['USER_NAME'].mode()[0])\n",
        "df_isi['FAC_ID'] = df_isi['FAC_ID'].fillna(df_isi['FAC_ID'].mode()[0])\n",
        "df_isi['PUBLIC_DATE'] = df_isi['PUBLIC_DATE'].fillna(df_isi['PUBLIC_DATE'].mode()[0])\n",
        "\n",
        "# Replace missing values with median values\n",
        "df_isi['STAFF_ID'] = df_isi['STAFF_ID'].fillna(df_isi['STAFF_ID'].median())"
      ],
      "metadata": {
